<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ListView
        android:id="@+id/WHList"
        android:layout_width="match_parent"
        android:layout_height="450dp"

        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"

        android:layout_below="@+id/bWHReturn">

    </ListView>


    <Button
        android:id="@+id/bWHReturn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"


        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"

        android:layout_marginTop="5dp"
        android:layout_marginLeft="8dp"
        android:layout_marginBottom="5dp"
        android:text="Return" />

    <TextView
        android:id="@+id/textViewWH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/bWHReturn"
        android:layout_marginBottom="11dp"
        android:layout_marginRight="50dp"
        android:layout_marginLeft="50dp"
        android:textSize="24dp"

        android:layout_toEndOf="@+id/bWHReturn"
        android:layout_toRightOf="@+id/bWHReturn"
        android:text="My Pantries" />

    <Button
        android:id="@+id/bCreate_WH"
        android:layout_width="160dp"
        android:layout_height="80dp"
        android:layout_below="@+id/WHList"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="6dp"

        android:text="Add new pantry"
        android:textSize="16dp"/>


</RelativeLayout>
